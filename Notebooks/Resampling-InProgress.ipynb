    "import pandas as pd\n",
    "df = pd.read_csv('demand_index.csv')\n",
    "df['Week'] = pd.to_datetime(df['Week'])\n",
    "df['Month'] = df['Week'].dt.to_period('M')\n",
    "monthly_data = df.groupby('Month').sum()\n",
    "monthly_data.to_csv('demand_index2.csv')"
